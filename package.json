{
  "name": "rpg-story-engine",
  "version": "1.0.0",
  "description": "A role-playing story generation engine.",
  "repository": "https://github.com/Glinkis/rpg-story-engine.git",
  "author": "Victor Glind√•s <glinkis@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ts-node server",
    "start:watch": "nodemon",
    "build": "webpack --mode production",
    "build:watch": "webpack --mode development --watch",
    "lint": "eslint **/*.ts && prettier **/*.ts --check",
    "lint:fix": "eslint **/*.ts --fix && prettier **/*.ts --write",
    "heroku-postbuild": "yarn build"
  },
  "nodemonConfig": {
    "watch": [
      "server",
      "client"
    ],
    "ext": "ts,tsx"
  },
  "dependencies": {
    "@babel/core": "7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/preset-react": "7.7.4",
    "@babel/preset-typescript": "7.7.4",
    "@emotion/core": "10.0.22",
    "@emotion/styled": "10.0.23",
    "@sentry/node": "5.9.0",
    "@types/compression": "1.0.1",
    "@types/express": "4.17.2",
    "@types/node": "12.12.14",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/webpack": "4.41.0",
    "@typescript-eslint/eslint-plugin": "2.9.0",
    "@typescript-eslint/parser": "2.9.0",
    "babel-loader": "8.0.6",
    "chalk": "3.0.0",
    "compression": "1.7.4",
    "eslint": "6.7.1",
    "eslint-config-prettier": "6.7.0",
    "express": "4.17.1",
    "nodemon": "2.0.1",
    "prettier": "1.19.1",
    "randoma": "1.3.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-hot-loader": "4.12.18",
    "ts-node": "8.5.4",
    "typescript": "3.7.2",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-hot-server-middleware": "0.6.0"
  }
}
